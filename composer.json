{
  "name": "emartech/emarsys-magento2-extension",
  "description": "Magento2 integration for the Emarsys Marketing Platform",
  "license": "proprietary",
  "require": {
    "php": ">7.0.1",
    "magento/product-community-edition": "2.2.6",
    "zendframework/zend-uri": "^2.5.1"
  },
  "type": "magento2-module",
  "version": "1.0.0",
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Emartech\\Emarsys\\": ""
    }
  },
  "archive": {
    "exclude": [
      "dev",
      "tasks",
      "codeship*",
      "Makefile",
      "Marketing_events.md",
      "docs"
    ]
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  }
}
